{
    "name": "rubium/sitemap",
    "description": "Rubium Sitemap package for Laravel.",
    "homepage": "https://rubium.ru",
    "keywords": ["laravel", "php", "sitemap", "generator", "xml", "html", "google-news"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rumen Damyanov",
            "email": "r@alfamatter.com",
            "role": "Developer",
            "homepage": "https://darumen.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Rubium/laravel-sitemap/issues",
        "source": "https://github.com/Rubium/laravel-sitemap",
        "wiki": "https://github.com/Rubium/laravel-sitemap/wiki"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.15|^9.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0|^9.0",
        "orchestra/testbench": "^4.18",
        "laravel/framework": "^6.20.42"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "orchestra/testbench-core": "^4.0|^5.0|^6.0|^7.0",
        "php-coveralls/php-coveralls" : "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Rubium\\Sitemap\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rubium\\Sitemap\\SitemapServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
